{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "shk-app": {
            "root": "",
            "sourceRoot": "src",
            "projectType": "application",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "preserveSymlinks": true,
                        "aot": true,
                        "outputPath": "dist",
                        "index": "src/index.html",
                        "main": "src/main.ts",
                        "tsConfig": "src/tsconfig.app.json",
                        "polyfills": "src/polyfills.ts",
                        "assets": [
                            "src/assets",
                            "src/favicon.ico"
                        ],
                        "styles": [
                            "node_modules/bootstrap/dist/css/bootstrap.css",
                            "node_modules/@fullcalendar/common/main.min.css",
                            "node_modules/@fullcalendar/daygrid/main.min.css",
                            "node_modules/@fullcalendar/timegrid/main.min.css",
                            "node_modules/@fullcalendar/list/main.min.css",
                            "node_modules/primeng/resources/themes/saga-blue/theme.css",
                            "node_modules/primeng/resources/primeng.css",
                            "node_modules/primeicons/primeicons.css",
                            "node_modules/primeflex/primeflex.css",
                            "node_modules/dragula/dist/dragula.css",
                            "node_modules/quill/dist/quill.core.css",
                            "node_modules/quill/dist/quill.snow.css",
                            "css/icomoon/style.css",
                            "css/styles.css"
                        ],
                        "scripts": [
                            "node_modules/quill/dist/quill.js",
                            "node_modules/chart.js/dist/chart.js",
                            "node_modules/ace-builds/src/ace.js",
                            "node_modules/ace-builds/src/ext-modelist.js",
                            "node_modules/ace-builds/src/theme-kuroir.js",
                            "node_modules/ace-builds/src/mode-yaml.js",
                            "node_modules/ace-builds/src/mode-html.js",
                            "node_modules/ace-builds/src/mode-javascript.js",
                            "node_modules/ace-builds/src/mode-css.js",
                            "node_modules/ace-builds/src/mode-json.js",
                            "node_modules/ace-builds/src/mode-twig.js"
                        ],
                        "allowedCommonJsDependencies": [
                            "lodash",
                            "dragula",
                            "chart.js",
                            "rxjs/util/isNumeric",
                            "rxjs/internal/operators"
                        ]
                    },
                    "configurations": {
                        "production": {
                            "optimization": true,
                            "outputHashing": "none",
                            "sourceMap": false,
                            "namedChunks": true,
                            "aot": true,
                            "extractLicenses": true,
                            "vendorChunk": false,
                            "commonChunk": false,
                            "buildOptimizer": true,
                            "progress": true,
                            "fileReplacements": [
                                {
                                    "replace": "src/environments/environment.ts",
                                    "with": "src/environments/environment.prod.ts"
                                }
                            ],
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "4mb",
                                    "maximumError": "5mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "6kb"
                                }
                            ]
                        }
                    }
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "browserTarget": "shk-app:build"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "shk-app:build:production"
                        }
                    }
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "shk-app:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "src/test.ts",
                        "karmaConfig": "./karma.conf.js",
                        "polyfills": "src/polyfills.ts",
                        "tsConfig": "src/tsconfig.spec.json",
                        "scripts": [
                            "node_modules/quill/dist/quill.js"
                        ],
                        "styles": [
                            "src/styles.css",
                            "node_modules/quill/dist/quill.core.css",
                            "node_modules/quill/dist/quill.snow.css",
                            "node_modules/dragula/dist/dragula.css"
                        ],
                        "assets": [
                            "src/assets",
                            "src/favicon.ico"
                        ]
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "src/tsconfig.app.json",
                            "src/tsconfig.spec.json"
                        ],
                        "exclude": []
                    }
                }
            }
        },
        "shk-app-e2e": {
            "root": "e2e",
            "sourceRoot": "e2e",
            "projectType": "application",
            "architect": {
                "e2e": {
                    "builder": "@angular-devkit/build-angular:protractor",
                    "options": {
                        "protractorConfig": "./protractor.conf.js",
                        "devServerTarget": "shk-app:serve"
                    }
                },
                "lint": {
                    "builder": "@angular-devkit/build-angular:tslint",
                    "options": {
                        "tsConfig": [
                            "e2e/tsconfig.e2e.json"
                        ],
                        "exclude": []
                    }
                }
            }
        },
        "comments": {
